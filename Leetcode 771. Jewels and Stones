class Solution {
public:
    int numJewelsInStones(string jewels, string stones) {
        
        map<char , int>mp;

        for(auto i : stones){
            mp[i]++;
        }

        int count = 0;

        for(auto i : mp){

            for(int j = 0; j < jewels.size() ; j++){
                 
                 if(jewels[j] == i.first)
                 count += i.second;
            }
        }
        return count;
    }
};
